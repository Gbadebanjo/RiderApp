{
  "expo": {
    "name": "RidersRydePRO",
    "slug": "RidersRydePRO",
    "version": "1.0.0",
    "scheme": "ridersrydepro",
    "icon": "./app/assets/newRydeproLogo.png",
    "splash": {
      "image": "./app/assets/Logo Image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "We need to use Face ID to authenticate your identity."
      },
      "bundleIdentifier": "com.RidersRydePRO"
    },
    "extra": {
      "eas": {
        "projectId": "8ccd55a7-c729-4593-a8e5-6c6c1505f961"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.885225392728-lutft160jr5nb1q7b3o05gklt5g8u6r2"
        }
      ],
      "expo-asset"
    ],
    "android": {
      "package": "com.RidersRydePRO"
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
