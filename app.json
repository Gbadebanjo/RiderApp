{
  "expo": {
    "name": "RidersRydePRO",
    "slug": "RidersRydePRO",
    "version": "1.0.0",
    "scheme": "ridersrydepro",
    "splash": {
      "image": "./app/assets/Logo Image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "We need to use Face ID to authenticate your identity."
      },
      "bundleIdentifier": "com.RidersRydePRO"
    },
    "extra": {
      "eas": {
        "projectId": "b09a07e1-9948-4456-8547-3603de08c3d4"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.447373894859-38d2ev07f0obb5a3f6usf0ja8l6g4v81"
        }
      ],
      "expo-asset"
    ],
    "android": {
      "package": "com.RidersRydePRO"
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
